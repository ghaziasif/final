<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ghaziprofessional" namespace="ghaziprofessional" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Student" table="student" lazy="true" >
    <id name="Id" column="id" />
    <property name="Name">
      <column name="name" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Address">
      <column name="address" sql-type="varchar(45)" not-null="false" />
    </property>
    <property name="Phone">
      <column name="phone" sql-type="int(11)" not-null="false" />
    </property>
  </class>
</hibernate-mapping>